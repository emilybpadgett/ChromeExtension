{
 "manifest_version": 2,
 "name": "Custom Google Homepage",
 "description": "This extension changes background and font of Google homepage",
 "version": "1.0",
 "content_scripts": 
  [
  {
  "matches": ["https://www.google.com/*"],
  "css": ["main.css"]
  }
  ],

"page_action": 
 {
 "default_icon": "img.png",
 "default_title": "My custom google page!"
 }
}